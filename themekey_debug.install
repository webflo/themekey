/**
 * Implements hook_uninstall().
 */
function themekey_debug_uninstall() {
  // Remove variables
  db_query("DELETE FROM {variable} WHERE name LIKE 'themekey_debug_%%'");
  cache_clear_all('variables', 'cache');
}
